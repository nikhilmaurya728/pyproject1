{
    "version":2,
    "builds": [
        {
            "scr":"pyproject1/wsgi.py",
            "user": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

        },
        {
            "scr":"build_files.sh",
            "user": "@vercel/static-build",
            "config": { "distDir": "staticfiles_build" }
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "scr":"pyproject1/wsgi.py",
            "dest": "pyproject1 /wsgi.py"
        }
    ]
}